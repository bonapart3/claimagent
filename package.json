    "lucide-react": "^0.309.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "openai": "^4.24.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.49.3",
